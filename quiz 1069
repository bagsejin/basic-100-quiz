#include <stdio.h>
int main(void) {
    
    char a;
    scanf("%c", &a);
    switch(a)
    {
        case 65:
        printf("best!!!");
        break;
        case 66:
        printf("good!!");
        break;
        case 67:
        printf("run!");
        break;
        case 68:
        printf("slowly~");
        break;
        default:
        printf("what?");
    }
    
    return 0;
}
