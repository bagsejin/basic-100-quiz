#include <stdio.h>
int main(void) {
    
    int score;
    scanf("%d", &score);
    
    switch(score / 10)
    {
        case 10:
        case 9:
            putchar('A');
            break;
        case 8:
        case 7:
            putchar('B');
            break;
        case 6:
        case 5:
        case 4:
            putchar('C');
            break;
        default:
            putchar('D');
            break;
    }
    
    return 0;
}
